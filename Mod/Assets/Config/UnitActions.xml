<?xml version="1.0" encoding="ISO-8859-1" ?>
<mod id="Actions" name="Unit Actions">
	 
	<options id="Actions" file="UnitActions.ini">
		<section id="Units">
			<!-- Sentry Healing by Unofficial Patch -->
			<option id="SentryHealing" key="Sentry Healing" type="boolean" default="True"/>
			<option id="SentryHealingOnlyNeutral" key="Sentry Healing Only Neutral" type="boolean" default="True"/>
			<!-- Ignore Harmless Barbarians (with Great Wall) -->
			<option id="IgnoreHarmlessBarbarians" key="Ignore Harmless Barbarians" type="boolean" default="True"/>
		</section>
		<section id="Workers">
			<!-- PreChop by EmperorFool -->
			<option id="PreChopForests" key="Pre Chop Forests" type="boolean" default="True"/>
			<option id="PreChopImprovements" key="Pre Chop Improvements" type="boolean" default="True"/>
		</section>
	</options>
</mod>
